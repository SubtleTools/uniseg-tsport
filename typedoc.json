{
  "entryPoints": ["src/index.ts"],
  "out": "./docs",
  "plugin": ["typedoc-plugin-markdown", "typedoc-theme-hierarchy"],
  "theme": "hierarchy",
  "readme": "./README.md",
  "name": "@tsports/uniseg",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "categorizeByGroup": true,
  "defaultCategory": "API",
  "categoryOrder": [
    "Grapheme",
    "Word",
    "Line",
    "Width",
    "Properties",
    "Utilities",
    "Types",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Document",
    "Project", 
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Method",
    "Parameter",
    "TypeParameter",
    "Accessor",
    "GetSignature",
    "SetSignature",
    "ObjectLiteral",
    "TypeLiteral",
    "CallSignature",
    "ConstructorSignature",
    "IndexSignature",
    "Reference"
  ]
}