  "excludes": [
    "dist/**",
    "node_modules/**",
    "test/automation/reference/**",
    "docs/**",
    "*.log",
    "*.lock",
    "package-lock.json",
    "bun.lock"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.19.3.wasm",
    "https://plugins.dprint.dev/markdown-0.17.8.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm"
  ]
}
